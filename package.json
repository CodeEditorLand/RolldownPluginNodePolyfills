{
	"description": "node polyfills for Rolldown",
	"devDependencies": {
		"@types/node": "22.2.0",
		"rolldown": "canary"
	},
	"files": [
		"dist/index.cjs",
		"dist/index.d.ts"
	],
	"main": "dist/index.cjs",
	"name": "@rolldown/plugin-node-polyfills",
	"scripts": {
		"build": "node scripts/build.js && tsc -p . && rolldown -c rolldown.config.mjs",
		"prebuild": "rm -rf dist",
		"prepublishOnly": "npm build"
	},
	"types": "dist/index.d.ts"
}
